{
  "name": "codefair",
  "version": "1.0.0",
  "private": true,
  "description": "Checks for License and Citation in a Github repo",
  "author": "slugb0t",
  "license": "MIT",
  "homepage": "https://github.com//",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev-old": "probot run ./index.js ",
    "dev": "nodemon --watch ./index.js --watch .env --watch ./package.json --watch ./utils/* --exec \"run-p dev:smee dev:start\"",
    "start": "node ./main.js",
    "dev:smee": "smee -u https://smee.io/n0NQN1LUpx2kI2T1 -p 3001",
    "dev:start": "node -r dotenv/config ./main.js",
    "build-css": "tailwindcss build -i ./public/assets/css/tailwind.css -o ./public/assets/css/styles.css",
    "watch-css": "npx tailwindcss build -i ./public/assets/css/tailwind.css -o ./public/assets/css/styles.css --watch",
    "build": "npm run build-css",
    "ts-build": "tsc",
    "dev-migrate": "tsx ./migrate.ts",
    "test": "jest"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "axios": "^1.6.8",
    "consola": "^3.2.3",
    "cwl-ts-auto": "^0.1.3",
    "dayjs": "^1.11.11",
    "humanparser": "^2.7.0",
    "js-yaml": "^4.1.0",
    "mongodb": "^6.5.0",
    "nanoid": "^5.0.7",
    "probot": "12.4.0",
    "url": "^0.11.3"
  },
  "type": "module",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "jest": "^29.0.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "smee-client": "^2.0.1",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@9.1.4",
  "engines": {
    "node": ">=20"
  },
  "package-manager-strict": false,
  "devEngines": {
    "packageManager": {
      "name": "pnpm",
      "version": ">= 9.0.0",
      "onFail": "warn"
    }
  }
}
