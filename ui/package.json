{
  "name": "codefair-app",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "tunnel": "lt --port 3000 --subdomain codefair",
    "build-full": "yarn prisma:migrate:deploy && nuxt build && yarn prisma:generate",
    "build": "nuxt build",
    "dev": "nuxt dev -p 3000",
    "start": "node .output/server/index.mjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev --preview-feature",
    "prisma:studio": "prisma studio",
    "prisma:db:push": "prisma db push",
    "prisma:db:pull": "prisma db pull",
    "scripts:migrate:mongodb": "tsx ./scripts/prisma-migrate.ts",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lint:fix": "prettier --write --list-different . && yarn lint:js --fix",
    "prune:branches": "npx git-removed-branches --prune --force",
    "reset": "rm -rf node_modules && rm -rf .nuxt && rm -rf .output && rm -rf dist && yarn install",
    "test": "jest",
    "analytics": "tsx ./scripts/analytics.ts"
  },
  "dependencies": {
    "@lucia-auth/adapter-mongodb": "^1.0.3",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@pinia/nuxt": "^0.5.3",
    "@prisma/client": "^5.19.1",
    "@tanstack/vue-table": "^8.21.2",
    "@vueuse/core": "^13.0.0",
    "arctic": "^1.6.2",
    "badge-maker": "^4.1.0",
    "dayjs": "^1.11.11",
    "doi-regex": "^0.1.17",
    "humanparser": "^2.7.0",
    "js-yaml": "^4.1.0",
    "lucia": "^3.2.0",
    "lucide-vue-next": "^0.487.0",
    "md-editor-v3": "4.20.2",
    "motion-v": "^1.0.0-beta.0",
    "notivue": "2.4.5",
    "nuxt": "3.13.1",
    "nuxtjs-naive-ui": "^1.0.2",
    "octokit": "^4.0.2",
    "pinia": "^2.2.2",
    "prisma": "^5.19.1",
    "random-words": "^2.0.1",
    "sanitize-html": "^2.13.0",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "vue": "^3.4.21",
    "vue-countup-v3": "^1.4.2",
    "vue-router": "^4.3.0",
    "yaml": "^2.4.5",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@inspira-ui/plugins": "^0.0.1",
    "@nuxtjs/color-mode": "3.4.1",
    "@nuxtjs/device": "3.1.1",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@nuxtjs/google-fonts": "3.2.0",
    "@nuxtjs/tailwindcss": "6.12.0",
    "@tailwindcss/typography": "0.5.13",
    "@types/doi-regex": "^0.1.3",
    "@types/sanitize-html": "2.11.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs-nuxt": "^2.1.9",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sort-keys-custom-order": "1.0.5",
    "eslint-plugin-unused-imports": "3.1.0",
    "git-removed-branches": "^2.3.1",
    "jest": "^29.7.0",
    "localtunnel": "^2.0.2",
    "mongodb": "^6.9.0",
    "naive-ui": "^2.40.0",
    "nock": "^13.5.4",
    "npm-run-all": "^4.1.5",
    "nuxt-icon": "0.6.10",
