{
  "name": "codefair",
  "version": "2.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "tunnel": "lt --port 3000 --subdomain codefair",
    "build": "nuxt build && yarn prisma:generate",
    "dev": "nuxt dev -p 3000",
    "start": "node .output/server/index.mjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev --preview-feature",
    "prisma:studio": "prisma studio",
    "prisma:db:push": "prisma db push",
    "prisma:db:pull": "prisma db pull",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "pnpm lint:js && pnpm lint:prettier",
    "lint:fix": "prettier --write --list-different . && pnpm lint:js --fix",
    "test": "jest"
  },
  "dependencies": {
    "@lucia-auth/adapter-mongodb": "^1.0.3",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@pinia/nuxt": "^0.5.3",
    "@prisma/client": "^5.19.1",
    "arctic": "^1.6.2",
    "dayjs": "^1.11.11",
    "humanparser": "^2.7.0",
    "js-yaml": "^4.1.0",
    "lucia": "^3.2.0",
    "md-editor-v3": "^4.13.5",
    "notivue": "^2.4.4",
    "nuxt": "3.11.1",
    "octokit": "^4.0.2",
    "pinia": "^2.2.2",
    "prisma": "^5.19.1",
    "random-words": "^2.0.1",
    "sanitize-html": "^2.13.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "yaml": "^2.4.5",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@bg-dev/nuxt-naiveui": "1.13.3",
    "@nuxtjs/color-mode": "3.4.1",
    "@nuxtjs/device": "3.1.1",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@nuxtjs/google-fonts": "3.2.0",
    "@nuxtjs/tailwindcss": "6.12.0",
    "@tailwindcss/typography": "0.5.13",
    "@types/sanitize-html": "2.11.0",
    "autoprefixer": "10.4.19",
    "dayjs-nuxt": "^2.1.9",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-sort-keys-custom-order": "1.0.5",
    "eslint-plugin-unused-imports": "3.1.0",
    "jest": "^29.7.0",
    "localtunnel": "^2.0.2",
    "nock": "^13.5.4",
    "npm-run-all": "^4.1.5",
    "nuxt-icon": "0.6.10",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss-debug-screens": "^2.2.1"
  },
